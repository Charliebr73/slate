<!DOCTYPE html>
<html>
<head>
  <title>Slate</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<% flash.each do |key, value| %>
  <div class="alert <%= flash_class(key) %>"><button type="button" class="close" data-dismiss="alert">&times;</button><%= value %></div>
<% end %>
<%= yield %>

</body>
</html>
